FoamFile
{
    version 2.0;
    format ascii;

    root "/home2/mnordlun/OpenFOAM/mnordlun-1.4.1/run/pmiCases";
    case "SMAR_MD2_Puffing";
    instance "0";
    local "";

    class volScalarField;
    object T;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298.15;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type inletOutlet;
        inletValue $internalField;
    }
    lowerWall
    {
        type zeroGradient;
    }
    upperWall
    {
        type zeroGradient;
    }
}


