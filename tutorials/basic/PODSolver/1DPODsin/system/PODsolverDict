FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          PODsolverDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type       scalarTransport;

//solver     RK;
solver     Euler;

eps        0.0001;

scalarTransportCoeffs
{
    field      T;
    accuracy   0.9999;

    velocity   U;
}


